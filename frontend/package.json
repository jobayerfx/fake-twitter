  "private": true,
  "scripts": {
    "dev": "node ./node_modules/nuxt/bin/nuxt.js -H=0.0.0.0 -p=3001",
    "build": "node --openssl-legacy-provider ./node_modules/nuxt/bin/nuxt.js build",
    "start": "node --openssl-legacy-provider ./node_modules/nuxt/bin/nuxt.js start",
    "generate": "node --openssl-legacy-provider ./node_modules/nuxt/bin/nuxt.js generate",
    "lint": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "format": "prettier --check .",
    "test": "npm run lint && npm run format",
    "fix": "prettier --write --list-different . && npm run lint -- --fix"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1624817847.21691f1",
