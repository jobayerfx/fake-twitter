    "cookie-universal-nuxt": "^2.2.2",
    "moment-timezone": "^0.5.34",
    "nuxt": "^2.16.3",
    "typescript": "4.0.5",
    "vue": "^2.7.10",
    "vue-debounce": "^3.0.1",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10",
    "vue-toastification": "^1.7.14",
    "vue2-editor": "^2.10.3",
    "vuetify": "^2.6.12",
    "vuex": "^3.6.2",
    "webpack": "^4.46.0"
